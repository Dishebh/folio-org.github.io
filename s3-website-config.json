{
  "ErrorDocument": {
    "Key": "error.html"
  },
  "IndexDocument": {
    "Suffix": "index.html"
  },
  "RoutingRules": [
    {
      "Condition": {
        "KeyPrefixEquals": "api"
      },
      "Redirect": {
        "ReplaceKeyWith": "reference/api"
      }
    },
    {
      "Condition": {
        "KeyPrefixEquals": "doc/api"
      },
      "Redirect": {
        "ReplaceKeyWith": "reference/api"
      }
    },
    {
      "Condition": {
        "KeyPrefixEquals": "doc/any-programming-language"
      },
      "Redirect": {
        "ReplaceKeyWith": "guides/any-programming-language"
      }
    },
    {
      "Condition": {
        "KeyPrefixEquals": "doc/artifacts"
      },
      "Redirect": {
        "ReplaceKeyWith": "download/artifacts"
      }
    },
    {
      "Condition": {
        "KeyPrefixEquals": "doc/glossary"
      },
      "Redirect": {
        "ReplaceKeyWith": "reference/glossary"
      }
    },
    {
      "Condition": {
        "KeyPrefixEquals": "doc/release-procedures"
      },
      "Redirect": {
        "ReplaceKeyWith": "guidelines/release-procedures"
      }
    },
    {
      "Condition": {
        "KeyPrefixEquals": "curriculum/"
      },
      "Redirect": {
        "ReplaceKeyWith": "tutorials/curriculum/"
      }
    }
  ]
}
